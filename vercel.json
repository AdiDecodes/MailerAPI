{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/sendmail",
        "methods": ["POST"],
        "dest": "./index.js"
      },
      {
        "src": "/(.*)",
        "dest": "./index.js"
      }
    ]
  }
  